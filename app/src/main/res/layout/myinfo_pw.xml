<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/Myprofile_Pw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="123dp"
        android:layout_marginLeft="123dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="65dp"
        android:layout_marginRight="65dp"
        android:layout_marginBottom="500dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Myprofile_Pw2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/Myprofile_Pw"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Myprofile_Pw"
        app:layout_constraintTop_toBottomOf="@+id/Myprofile_Pw" />

    <Button
        android:id="@+id/Myprofile_PwBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="191dp"
        android:layout_marginLeft="191dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="201dp"
        android:layout_marginRight="201dp"
        android:layout_marginBottom="308dp"
        android:text="변경완료"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="변경할 비밀번호"
        app:layout_constraintBottom_toBottomOf="@+id/Myprofile_Pw"
        app:layout_constraintEnd_toStartOf="@+id/Myprofile_Pw"
        app:layout_constraintTop_toTopOf="@+id/Myprofile_Pw" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:text="비밀번호 확인"
        app:layout_constraintBottom_toBottomOf="@+id/Myprofile_Pw2"
        app:layout_constraintEnd_toStartOf="@+id/Myprofile_Pw2"
        app:layout_constraintTop_toTopOf="@+id/Myprofile_Pw2" />

    <TextView
        android:id="@+id/Myprofile_PwErr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/Myprofile_Pw2"
        app:layout_constraintStart_toStartOf="@+id/Myprofile_Pw2"
        app:layout_constraintTop_toBottomOf="@+id/Myprofile_Pw2" />

</androidx.constraintlayout.widget.ConstraintLayout>